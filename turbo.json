{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev:core": {
      "dependsOn": ["@tradeb0t/core#dev", "stubb0t#dev"]
    },
    "dev:dashboard": {
      "dependsOn": ["@tradeb0t/core#dev", "stubb0t#dev", "@tradeb0t/dashboard#dev"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "lint": {},
    "lintfix": {},
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "@tradeb0t/core#dev": {},
    "stubb0t#dev": {},
    "@tradeb0t/dashboard#dev": {}
  }
}
